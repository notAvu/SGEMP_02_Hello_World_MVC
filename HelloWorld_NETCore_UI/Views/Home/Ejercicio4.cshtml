
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Ejercicio4</title>
</head>
<body>
    <table border="1" cellpadding="2">
        @for (var m = 1; m < 10; m++)
        {
            <tr>
                @for (var i = 1; i < 10; i++)

                {
                    var x = @i * @m; <td>@m x @i= (x)</td>
                    }
            </tr>
        }

    </table>

</body>
</html>
